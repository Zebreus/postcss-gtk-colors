{
  "name": "postcss-plugin",
  "version": "0.0.0",
  "description": "PostCSS plugin gnome",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-plugin"
  ],
  "scripts": {
    "test": "jest --coverage && eslint .",
    "generate-parser": "peggy -o src/parsers/cssCalcParser.js src/parsers/cssCalcParser.peggy --format commonjs"
  },
  "author": "Lennart Eichhorn <lennart@madmanfred.com>",
  "license": "MIT",
  "repository": "zebreus/postcss-plugin",
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.3",
    "@types/jest": "^28.1.1",
    "@types/typescript": "^2.0.0",
    "clean-publish": "^3.4.2",
    "css-validator": "^0.11.0",
    "eslint": "^8.0.1",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^28.1.1",
    "peggy": "^2.0.1",
    "postcss": "^8.3.11",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "dependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/color-string": "^1.5.2",
    "color-convert": "^2.0.1",
    "color-string": "^1.9.1",
    "minify-css-calc": "^0.0.8"
  }
}
